<yandex>
    <storage_configuration>
        <disks>
            <hot>
                <path>/data/cold/</path>
                <keep_free_space_bytes>1073741824</keep_free_space_bytes>
            </hot>
            <cold_st1>
                <path>/data/cold_st1/</path>
                <keep_free_space_bytes>10737418240</keep_free_space_bytes>
            </cold_st1>
            <cold_st1_two>
                <path>/data/cold_st1_2/</path>
                <keep_free_space_bytes>10737418240</keep_free_space_bytes>
            </cold_st1_two>
            <cold_st1_three>
                <path>/data/cold_st1_3/</path>
                <keep_free_space_bytes>10737418240</keep_free_space_bytes>
            </cold_st1_three>
            <cold_sc1>
                <path>/data/cold_sc1/</path>
                <keep_free_space_bytes>10737418240</keep_free_space_bytes>
            </cold_sc1>
        </disks>
        <policies>
        <default>
            <volumes>
                <hot>
                    <disk>hot</disk>
                </hot>
                <cold_st1>
                    <disk>cold_st1</disk>
                    <disk>cold_st1_two</disk>
                    <disk>cold_st1_three</disk>
                    <prefer_not_to_merge>True</prefer_not_to_merge>
                </cold_st1>
            </volumes>
            <move_factor>0.10</move_factor>
        </default>
        <hot_to_cold>
            <hot>
                <disk>hot</disk>
                <!--<max_data_part_size_bytes>50000000000</max_data_part_size_bytes>-->
            </hot>
            <cold_st1>
                <disk>cold_st1</disk>
                <disk>cold_st1_two</disk>
                <disk>cold_st1_three</disk>
                <prefer_not_to_merge>True</prefer_not_to_merge>
            </cold_st1>
            <cold_sc1>
                <disk>cold_sc1</disk>
                <prefer_not_to_merge>True</prefer_not_to_merge>
            </cold_sc1>
            </volumes>
            <move_factor>0.10</move_factor>
        </hot_to_cold>
        <cold_to_st1>
            <volumes>
                <cold_st1>
                    <disk>cold_st1</disk>
                    <disk>cold_st1_two</disk>
                </cold_st1>
            </volumes>
            <move_factor>0.01</move_factor>
        </cold_to_st1>
        <st1>
        <policies>
            <default>
                <volumes>
                    <hot>
                        <disk>hot</disk>
                    </hot>
                    <cold_st1>
                        <disk>cold_st1</disk>
                        <disk>cold_st1_two</disk>
                        <disk>cold_st1_three</disk>
                        <prefer_not_to_merge>True</prefer_not_to_merge>
                    </cold_st1>
                </volumes>
                <move_factor>0.10</move_factor>
            </default>
            <hot_to_cold>
            <volumes>
                <cold_st1>
                    <disk>cold_st1</disk>
                    <disk>cold_st1_two</disk>
                </cold_st1>
            </volumes>
            <move_factor>0.01</move_factor>
            </cold_to_st1>
            <st1>
                <volumes>
                    <cold_st1>
                        <disk>cold_st1</disk>
                        <disk>cold_st1_two</disk>
                        <disk>cold_st1_three</disk>
                    </cold_st1>
                </volumes>
            </st1>
            <st1_to_sc1>
                <volumes>
                    <cold_st1>
                        <disk>cold_st1</disk>
                        <disk>cold_st1_two</disk>
                        <disk>cold_st1_three</disk>
                    </cold_st1>
                    <cold_sc1>
                        <disk>cold_sc1</disk>
                        <prefer_not_to_merge>1</prefer_not_to_merge>
                    </cold_sc1>
                </volumes>
                <move_factor>0.10</move_factor>
            </st1_to_sc1>
        </policies>
    </storage_configuration>
</yandex>